{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2020",
    "lib": [
      "es2017",
      "es2019"
    ],
    "importHelpers": false,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "sourceMap": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "baseUrl": "."
  }
}